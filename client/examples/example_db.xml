<?xml version="1.0" encoding="UTF-8"?>
<uml name="example_db">
    <classes>
        <class name="Person" x="100" y="100" id="class#1">
            <attributes>
                <attribute>name</attribute>
                <attribute>age</attribute>
            </attributes>
            <operations>
                <operation>setName()</operation>
                <operation>getName()</operation>
                <operation>setAge()</operation>
                <operation>getAge()</operation>
            </operations>
        </class>
        <class name="Car" x="200" y="200" id="class#2">
            <attributes>
                <attribute>brand</attribute>
                <attribute>model</attribute>
            </attributes>
            <operations>
                <operation>settBrand()</operation>
                <operation>getBrand()</operation>
                <operation>getModel()</operation>
                <operation>setModel()</operation>
            </operations>
        </class>
    </classes>
    <relationships>
        <relationship type="realization" id="">
            <source>class#1</source>
            <destination>class#2</destination>
        </relationship>
    </relationships>
</uml>